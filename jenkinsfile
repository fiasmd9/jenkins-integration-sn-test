                            steps {
                                echo "S3..."
                            }
                        }
                        stage("S4") {
                            steps {
                                echo "S4..."
                            }
                        }
                    }
                }
                stage("S5") {
                    steps {
                        echo "S5..."
                    }
                }
            }
        }
        stage("S6") {
            steps {
            	echo "S6..."
            }
        }
    }
}
