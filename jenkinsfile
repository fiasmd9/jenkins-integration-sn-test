pipeline {
    agent any
    stages {
        stage('Example') {
            steps {
                echo 'Hello World'
            }
            
        
            
            post { 
                always { 
        script { 
                 def server = Artifactory.server 'JFROG1'
                 def downSpec = """{
                        "files": [{
                            "pattern": "default-docker-local/com/bnc/app-devops-jenkins-1.33.2-SNAPSHOT.jar",
                            "target": "/tmp/"
                        }]
                    }"""

              def buildInfo = server.download(downSpec)
                    server.publishBuildInfo buildInfo
                }
            }
        }
        }
    


        stage('Stage2') {
            steps {
            echo "stage2"
            }
        }
    }

}
