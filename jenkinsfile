pipeline {
    agent any
    stages {
        stage("S0") {
            steps {
            	echo "S0..."
            }
        }
        stage("S1") {
            stages {
                stage("S2") {
                    steps {
                        snDevOpsChange()
                        echo "S2..."
                    }
                }
            }
        }
        stage("S3") {
            parallel {
                stage("S4") {
                    steps {
                        echo "S4..."
                    }
                }
                stage("S5") {
                    steps {
                        snDevOpsChange()
                        echo "S5..."
                    }
                }
                stage("S6") {
                    steps {
                        echo "S6..."
                    }
                }
            }
        }
        stage("S7") {
